# .env - Переменные окружения для CryptoNow

# Webpack Dev Server
WEBPACK_DEV_SERVER_HOST=localhost
WEBPACK_DEV_SERVER_PORT=19006

# Development settings
NODE_ENV=development
DEBUG=true

# Expo settings (для совместимости)
EXPO_PUBLIC_USE_STATIC=true

# Browser settings
BROWSER=chrome

# Отключаем некоторые проверки для разработки
GENERATE_SOURCEMAP=true
SKIP_PREFLIGHT_CHECK=true

# Solana network (devnet для разработки)
REACT_APP_SOLANA_NETWORK=devnet
REACT_APP_SOLANA_RPC_URL=https://api.devnet.solana.com